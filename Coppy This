const Asena = require('../events');
const Config = require('../config');
const {MessageType,Mimetype} = require('@adiwajshing/baileys');
const Language = require('../language');
const Lang = Language.getString('_asena');
const axios = require('axios');



Asena.addCommand({pattern: 'tenuh', fromMe: true, deleteCommand: false,}, (async (message, match) => {

        var respoimage = await axios.get(`https://telegra.ph/file/2dfaa2a9c622b3df56e72.jpg`, { responseType: 'arraybuffer' })

        await message.client.sendMessage(message.jid, Buffer.from(respoimage.data), MessageType.image, {mimetype: Mimetype.png, caption: ' hellow world\n\n '})
    }));
  
if (Config.WORKTYPE == 'public') {
  
Asena.addCommand({pattern: 'tenuh', fromMe: false, deleteCommand: false,}, (async (message, match) => {

        var respoimage = await axios.get(`https://telegra.ph/file/2dfaa2a9c622b3df56e72.jpg`, { responseType: 'arraybuffer' })

        await message.client.sendMessage(message.jid, Buffer.from(respoimage.data), MessageType.image, {mimetype: Mimetype.png, caption: '  hellow world\n\n '})
    }));
}
